            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">

                <!-- /.search form -->
                <!-- Sidebar Menu -->
                @if (Session["list1"] != null)
                {
                <ul class="sidebar-menu" data-widget="tree">

                    @foreach (var item in (List
                    <HealthCareMvc5.LogInServiceReference.Login1>
                        )Session["list"])
                        {
                        if (item.title == "Tracker")
                        {
                        var Entityname = @item.title.Replace(" ", "_");
                        <li class="treeview" id="lst-@item.title.Replace(" ", " _")">
                            <a href="#">
                                <i class="" id="@item.title.Replace(" ", " _")"></i>
                                <span>Tracker</span>
                                <span class="pull-right-container">
                                    <i class="fa fa-envelope-o pull-right"></i>
                                </span>
                            </a>
                            <ul class="treeview-menu">
                                @foreach (var item1 in (List
                                <HealthCareMvc5.LogInServiceReference.Login1>
                                    )Session["list1"])
                                    {

                                    if (item.title == item1.title)
                                    {
                                    if (@item1.Stitle == "ClientIssueApproval")
                                    {

                                    <li id="lstnxt-@item1.Stitle">
                                        <a href="#" id="@item1.Stitle" onclick="clickevent('Tracker','@item1.Stitle',this);">

                                            <i class="fa fa-circle"></i>
                                            <span>
                                                Client Issue Approval
                                            </span>
                                        </a>
                                    </li>
                                    }


                                    }
                                    if (item.title == item1.title)
                                    {
                                    if (@item1.Stitle == "IssueApproval")
                                    {

                                    <li id="lstnxt-@item1.Stitle">
                                        <a href="#" id="@item1.Stitle" onclick="clickevent('Tracker','@item1.Stitle',this);">

                                            <i class="fa fa-circle"></i>
                                            <span>
                                                Issue Approval
                                            </span>
                                        </a>
                                    </li>
                                    }


                                    }
                                    if (item.title == item1.title)
                                    {
                                    if (@item1.Stitle == "ClientIssueRaise")
                                    {

                                    <li id="lstnxt-@item1.Stitle">
                                        <a href="#" id="@item1.Stitle" onclick="clickevent('Tracker','@item1.Stitle',this);">
                                            <i class="fa fa-circle"></i>
                                            <span>
                                                Client Issue Raise
                                            </span>
                                        </a>
                                    </li>
                                    }


                                    }

                                    }
                            </ul>
                        </li>
                        }
                        }

                </ul>
                }
                else
                {
                Response.Redirect(Url.Action("SessionExpiry", "Login"));

                }


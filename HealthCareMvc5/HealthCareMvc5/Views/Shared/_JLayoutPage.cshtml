    <div class="modal fade" id="ChangePassword" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog" style="width: 75%">
            <div class="modal-content" style="width: 300px; margin: 30px auto;margin-top:-5%">
                <div class="modal-header" style="background-color: #00766c">
                    <button type="button" class="close" data-dismiss="modal">
                        ×
                    </button>
                    <h4 class="modal-title" style="color: #FFF;">
                        Change Password
                    </h4>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-sm-12">
                            <label>
                                User Name
                            </label>
                            <input type="text" id="username" value="@Session["UserName"]"
                                   class="form-control" disabled="disabled" />
                            <input type="hidden" id="userid" value="@Session["UserId"]"
                                   class="form-control" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12">
                            <label>
                                Old Password
                            </label>
                            <input type="password" id="oldpsw" class="form-control" autocomplete="off" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12">
                            <label>
                                New Password
                            </label>
                            <input type="password" id="newpsw" class="form-control" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12">
                            <label>
                                Confirm Password
                            </label>
                            <input type="password" id="cnfmpsw" class="form-control" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12" align="right" style="margin-top: 8px;">
                            <button type="button" class="btn btn-primary" data-dismiss="modal"
                                    id="updatepsw">
                                Update
                            </button>
                            <button type="button" class="btn btn-warning" data-dismiss="modal"
                                    id="cancelpsw">
                                Cancel
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



    <script type="text/javascript">
        $("#ChangePassword").modal("hide");
        $("#ChngPsw").click(function () {
            //$("#ChangePassword").modal("show");
            $("#oldpsw").val("");
            $("#newpsw").val("");
            $("#cnfmpsw").val("");

        })
        
        $("#cancelpsw").click(function () {
            $("#oldpsw").val("");
            $("#newpsw").val("");
            $("#cnfmpsw").val("");
        })
        $(window).scroll(function () {

            if ($(this).scrollTop() >= 50) {        // If page is scrolled more than 50px
                $('#return-to-top').fadeIn(200);    // Fade in the arrow
            } else {
                $('#return-to-top').fadeOut(200);   // Else fade out the arrow
            }
        });
        $('#return-to-top').click(function () {      // When arrow is clicked
            $('body,html').animate({
                scrollTop: 0                       // Scroll to top of body
            }, 500);
        });

        $('#Tracker').removeClass('').addClass('fa fa-circle-o');


        function userimage() {
            $.ajax({
                url: "../Login/Getimage",
                data: {},
                type: 'POST',
                success: function (ImageBind) {
                    if (ImageBind != '') {
                        if (ImageBind != 'data:image/jpeg;base64,') {
                            $("#imguser").attr('src', '');
                            $("#imguser").attr('src', ImageBind);
                        }
                    }

                }

            });
        }
        $(document).ajaxStart(function () { Pace.restart(); });




        $("#Module").text('');
        $("#SubModule").text('');
        $("#MenuNamepartialview").text('');
        function clickevent(e, t, el, Stitle) {
            $("#bodytag").addClass("sidebar-collapse");
            $("#Strapid").hide();
            $("#MenuNamepartialview").text('');
            $("#Module").text('');
            $("#SubModule").text('');
            $("#ADDNEWPARTIALVIEW").html('');
            var Module = e;
            var Submodule = t.replace('_', '');
            $("#ButtonDivision").html('');
            $("#Mandatory").show();
            $(".sidebar-menu>li").removeClass("active");
            $("#lst-" + e).addClass('active');
            $(".sidebar-menu>li>ul>li").removeClass("active");
            $("#lstnxt-" + t).addClass("active");

            var chek = $(".sidebar-menu> .treeview > .treeview-menu > #" + e).parent();

            $.get("../" + Submodule + "/ADDNEWPARTIALVIEW", { cache: false, headers: { "Accept-Encoding": "gzip" } }, function (n) {
                $("#MenuNamepartialview").text(Stitle);
                $("#MenuNamepartialview").text(t.replace(/([A-Z])/g, ' $1').trim());
                $("#SubModule").text(t.replace(/([A-Z])/g, ' $1').trim());
                $("#Module").text(e.replace('_', ' '));
                $("#SubAct").show();
                var Class = $('#' + e).attr('class');
                $("#MainClass").addClass(Class);

                $("#ADDNEWPARTIALVIEW").html(n);




            });



        }
        $("#updatepsw").click(function () {
            debugger
            var UserId = $("#userid").val();
            var Oldpsw = $("#oldpsw").val();
            var Newpsw = $("#newpsw").val();
            var Cnfmpsw = $("#cnfmpsw").val();

            if (Oldpsw == "") {
                bootbox.alert({ size: "small", message:'Enter Old Password' });
                $("#oldpsw").focus();
                return false;
            }
            else if (Newpsw == "") {
                bootbox.alert({ size: "small", message: 'Enter New Password' });
                $("#newpsw").focus();
                return false;
            }
            else if (Oldpsw == Newpsw) {
                bootbox.alert({ size: "small", message: "Old Password and New Password should't be Same" });
                $("#newpsw").val("");
                $("#cnfmpsw").val("");
                $("#newpsw").focus();
                return false;
            }
            else if (Cnfmpsw == "") {
                bootbox.alert({ size: "small", message: "Enter New Confirm Password" })
                $("#cnfmpsw").val("");
                $("#cnfmpsw").focus();
                return false;
            }

            if (Newpsw != Cnfmpsw) {
                bootbox.alert({ size: "small", message: "New Password and Confirm Password should be Same" });
                $("#cnfmpsw").val("");
                $("#cnfmpsw").focus();
                return false;
            }
            else {
                $.ajax({
                    url: "../Login/UpdateUser",
                    type: "POST",
                    data: {
                        UserId: UserId, Password: Newpsw, ConfirmPassword: Cnfmpsw
                    },
                    success: function (data) {
                        debugger
                        bootbox.alert({ size: "small", message: 'Password Updated Successfully' });
                        $("#newpsw").val('');
                        $("#ChangePassword").hide();
                    }
                });
            }
        });

        $("#oldpsw").change(function () {
            var txtuser = $("#userid").val();
            var oldpsw = $("#oldpsw").val();
            $.ajax({
                url: "../Login/compare",
                type: 'POST',
                data: { txtuser: txtuser },
                success: function (data) {

                    if (oldpsw == data) {
                        return false;
                    }
                    else {
                        bootbox.alert({ size: "small", message: "Please Enter Correct Old Password" });
                        $("#oldpsw").val("");
                        $("#oldpsw").focus();
                    }
                }
            });
        });

    </script>
    @RenderSection("scripts", required: false)
</body>

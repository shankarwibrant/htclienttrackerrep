            <span class="hidden-xs">@Session["UserName"]</span>
        </a>*@
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#" style="text-transform:uppercase; height: 55px; padding:15px;">
                                <label style="margin-top: 10px;color: white !important;font-size: 20px !important;">@Session["UserName"]</label>

                                <img src='@Url.Action("Getimage", "Login", new { value = @Session["UserID"] })' id="imageObj" class="img-circle pull-right" style=" width: 45px; height: 55px; padding-top: 2px; padding-bottom: 2px;" />

                            </a>
